#include <iostream>
using namespace std;

int main() {
    int m1[2][2], m2[2][2], res[2][2];

    for (int i = 0; i < 2; i++)
        for (int j = 0; j < 2; j++) cin >> m1[i][j];
    for (int i = 0; i < 2; i++)
        for (int j = 0; j < 2; j++) cin >> m2[i][j];

    for (int i = 0; i < 2; i++)
        for (int j = 0; j < 2; j++) res[i][j] = m1[i][j] - m2[i][j];

    for (int i = 0; i < 2; i++) {
        for (int j = 0; j < 2; j++) cout << res[i][j] << " ";
        cout << endl;
    }

    return 0;
}
